{
  "name": "arietimmerman/laravel-url-shortener",
  "description": "Laravel Package for shortening urls with Laravel 10 and 11 support",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "url", "shortener", "redirect", "bitly", "tracking"],
  "homepage": "https://github.com/arietimmerman/laravel-url-shortener",
  "authors": [
    {
      "name": "Arie Timmerman",
      "email": "arietimmerman@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "webpatser/laravel-uuid": "^4.0",
    "illuminate/database": "^9.0|^10.0|^11.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "illuminate/console": "^9.0|^10.0|^11.0"
  },
  "classmap": [
    "database/migrations"
  ],
  "autoload": {
    "psr-4": {
      "ArieTimmerman\\Laravel\\URLShortener\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArieTimmerman\\Laravel\\URLShortener\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^7.0|^8.0|^9.0",
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit": "^9.0|^10.0",
    "illuminate/testing": "^9.0|^10.0|^11.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "ArieTimmerman\\Laravel\\URLShortener\\ServiceProvider"
      ],
      "aliases": {
        "URLShortener": "ArieTimmerman\\Laravel\\URLShortener\\URLShortener"
      }
    }
  }
}
